{
  "manifest_version": 2,
  "name": "rsstodolist",
  "version": "1.3.1",
  "description": "Send links to rsstodolist - https://rsstodolist.appspot.com/",
  "homepage_url": "https://github.com/paulgreg/rsstodolist-firefox",
  "icons": {
    "16": "imgs/rss.png",
    "48": "imgs/rss.png",
    "96": "imgs/rss.png",
    "128": "imgs/rss.png"
  },
  "permissions": [
    "activeTab",
    "notifications",
    "storage"
  ],
  "browser_action": {
    "default_icon": "imgs/rss.png",
    "default_title": "rsstodolist",
    "default_popup": "index.html"
  }
}
